{
    "servers": {
        "playwright-mcp": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "@playwright/mcp@latest"
            ]
        },
        "gaia": {
            "type": "stdio",
            "command": "dotnet",
            "args": [
                "run",
                "--project",
                ".gaia/mcps/gaia/src/fa.mcp.gaia/fa.mcp.gaia.csproj"
            ]
        },
        "LifeOS MCP": {
            "url": "http://localhost:5001/mcp",
            "type": "http"
        }
    },
    "inputs": []
}
